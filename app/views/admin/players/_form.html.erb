<%= form_for([:admin, @player]) do |f| %>
  <%= error_messages_for :player %>

  <!--[form:player]-->
  <ul class="form">
    <li><%= f.label :handle %>
      <%= f.text_field :handle, {disabled: !@player.new_record?} %></li>
    <li><%= f.label :passwd %>
      <%= f.password_field :passwd %></li>
    <li><%= f.label :city %>
      <%= f.text_field :city %></li>
    <li><%= f.label :state %>
      <%= f.text_field :state %></li>
    <li><%= f.label :country %>
      <%= f.text_field :country %></li>
    <li><%= f.label :email %>
      <%= f.text_field :email %></li>
    <li><%= f.label :bio %>
      <%= f.text_area :bio %></li>
  </ul>

  <%= submit_tag %>

  <!--[eoform:player]-->
<% end %>